  "repository": {
    "type": "git",
    "url": "https://github.com/justinlettau/standard-rest-response"
  },
  "bugs": {
    "url": "https://github.com/justinlettau/standard-rest-response/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "files": [
    "dist"
  ],
  "scripts": {
