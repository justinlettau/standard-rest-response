    "@typescript-eslint/parser": "^4.16.1",
    "del-cli": "^3.0.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "standard-version": "^9.2.0",
    "ts-jest": "^26.5.2",
    "typescript": "^4.2.2"
